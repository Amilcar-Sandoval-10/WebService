@model List<WebFacturaMvc.Models.ViewModels.ListTablaViewModel>
    @{
        ViewBag.Title = "Lista de Usuarios";
    }
    <a href="~/Reportes/wfReport.aspx">Reporte</a>
    <h2 class="text-center text-success">Listado de Usuarios</h2>
    <div class="well well-sm"><a class="btn btn-primary" href="~/Table/Nuevo">Nuevo</a></div>
    <table class="table table-hover table-striped">
        <thead>
            <tr class="bg-primary">
                <th>id_usuario</th>
                <th>nombre</th>
                <th>emai</th>
                <th>password</th>
                <th>fecha</th>
                <th>id_rol</th>
                <th></th>
                <th colspan="3">Operaciones</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var oElemento in Model)
            {
                <tr>

                    <td>@oElemento.id_usuario</td>
                    <td>@oElemento.nombre</td>
                    <td>@oElemento.email</td>
                    <td>@oElemento.password</td>
                    <td>@oElemento.fecha</td>
                    <td>@oElemento.id_rol</td>
                    <td><a class="btn btn-success" href="~/Table/Editar/@oElemento.id_usuario"><span class="glyphicon glyphicon-eye-open"></span></a></td>
                    <td><a class="btn btn-info" href="~/Table/Eliminar/@oElemento.id_usuario"><span class="fa fa-edit"></span></a></td>

                </tr>
            }
        </tbody>

    </table>
